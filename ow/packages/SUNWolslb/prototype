f none openwin/lib/libce.a			0755 root bin
f none openwin/lib/libolgx.a			0755 root bin
f none openwin/lib/libxview.a			0755 root bin


!search      $HOME/lib
f none openwin/lib/llib-lXol.ln			0755 root bin
f none openwin/lib/llib-lolgx.ln		0755 root bin


!search      $HOME/lib/libp
d none openwin/lib/libp				0775 root bin
f none openwin/lib/libp/libXol.a		0755 root bin
f none openwin/lib/libp/libolgx.a		0755 root bin
f none openwin/lib/libp/libxview.a		0755 root bin

