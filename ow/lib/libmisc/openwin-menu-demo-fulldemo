"Demos" TITLE PIN

"Icosahedron..."	exec $OPENWINHOME/demo/ico2
"Maze..."		exec $OPENWINHOME/demo/maze
"Muncher..."	   	exec $OPENWINHOME/demo/muncher
"Plaid..."		exec $OPENWINHOME/demo/plaid
"Puzzle..."		exec $OPENWINHOME/demo/puzzle
"Worms..."		exec $OPENWINHOME/demo/worm
"Xsol..."		exec $OPENWINHOME/demo/xsol
"Spider..."             exec $OPENWINHOME/demo/spider
"OLIT Sampler..."       exec $OPENWINHOME/demo/olitsampler
