ptype Sun_Imagetool_Sun_Raster {
    start "$OPENWINHOME/bin/imagetool -tooltalk";
    per_session 5;
  handle:
/*
 * Session declarations. 
 */

/*
 * Display for Sun Raster media type
 */

    session Display (in Sun_Raster rasterdata) => start;
    session Display (in Sun_Raster rasterdata, in title text) => start;
    session Display (in Sun_Raster rasterdata, in messageID text) => start;
    session Display (in Sun_Raster rasterdata, in messageID text, in title text) => start;

    file Display (in Sun_Raster rasterdata) => start;
    file Display (in Sun_Raster rasterdata, in title text) => start;
    file Display (in Sun_Raster rasterdata, in messageID text) => start;
    file Display (in Sun_Raster rasterdata, in messageID text, in title text) => start;

 };

